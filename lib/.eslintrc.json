{
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "rules": {
        "react/display-name": "off",
        "react/prop-types": "off",
        "@typescript-eslint/unbound-method": "off"
    }
}